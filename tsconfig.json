{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES6"],
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "isolatedModules": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": false,
    "strict": true,
    "strictNullChecks": true,
    "incremental": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
