{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES6"],
    "module": "ES6",
    "moduleResolution": "Node",
    "isolatedModules": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "strict": true,
    "strictNullChecks": true,
    "incremental": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
